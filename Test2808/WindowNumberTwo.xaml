<Window x:Class="Test2808.WindowNumberTwo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Test2808"
        mc:Ignorable="d"
        Title="WindowNumberTwo" Height="300" Width="300">
    <Grid>
        <Label Content="Width" HorizontalAlignment="Left" Margin="25,36,0,0" VerticalAlignment="Top" Height="28" Width="60"/>
        <Label Content="Height" HorizontalAlignment="Left" Margin="25,85,0,0" VerticalAlignment="Top" Height="28" Width="60"/>
        <Label Content="Text" HorizontalAlignment="Left" Margin="25,147,0,0" VerticalAlignment="Top" Height="28" Width="60"/>
        <TextBlock HorizontalAlignment="Left" Margin="110,36,0,0" TextWrapping="Wrap" Text="{Binding WidthBtn ,Mode=TwoWay}" VerticalAlignment="Top" Height="28" Width="114"/>
        <TextBlock HorizontalAlignment="Left" Margin="110,85,0,0" TextWrapping="Wrap" Text="{Binding HeightBtn ,Mode=TwoWay}" VerticalAlignment="Top" Height="28" Width="114"/>
        <TextBlock HorizontalAlignment="Left" Margin="110,147,0,0" TextWrapping="Wrap" Text="{Binding TextBtn ,Mode=TwoWay}" VerticalAlignment="Top" Height="28" Width="114"/>

    </Grid>
</Window>
